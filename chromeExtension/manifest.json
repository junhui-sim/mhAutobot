  "version": "2.1.14",
  "author": "NobodyRandom",
  "update_url": "https://raw.githubusercontent.com/nobodyrandom/mhAutobot/master/chromeExtensionUpdate.xml",
  "icons": {
	"16": "mice16.png",
	"32": "mice32.png",
	"40": "mice.png",
	"48": "mice48.png",
	"128": "mice128.png"
